package org.foobarspam.pulpo;

public class Correlacion {

	public Correlacion() {
	}
	//Creamos una tabla con los 4 numeros
	Object tabla[][] = {{76,9,4,1}};
	Object[][] phi = tabla;
	
	public double phi(){
		int matriz[][];
		matriz = new int[4][4];
		for (int x=0; x < matriz.length; x++) {
		  for (int y=0; y < matriz[x].length; y++) {
		      System.out.println (matriz[x][y]);
		  }
		return 0;
		
	}
}
